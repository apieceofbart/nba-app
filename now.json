{
  "version": 2,
  "name": "nba-app",
  "builds": [
    { "src": "src/backend/index.ts", "use": "@now/node@canary" },
    { "src": "src/frontend/index.html", "use": "@now/static" }
  ],
  "routes": [{ "src": "/api/", "dest": "src/backend/index.ts" }, { "src": "/.*", "dest": "src/frontend/index.html" }],
  "alias": "nba-app.now.sh"
}
